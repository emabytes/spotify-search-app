<%-include('./partials/head')%>

<section id="artist-list">
    <% spotifyData.forEach(elt => { %>
    <article>
        <div>
            <% if(elt.images[0] != undefined){%>
            <img src="<%= elt.images[0].url%> " alt="">
            <%} else {%>
            <img src="empty.png" alt="album cover">
            <% } %>
        </div>
        <h3> <%= elt.name %> </h3>
        <a class="btn" href="/albums/<%= elt.id %>">View Albums</a>
    </article>
    <% }) %>
</section>
</body>

</html>